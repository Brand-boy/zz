	    print "[!] Invalid Username."
	    time.sleep(1)
	    tlogin()
	    
	passw = raw_input("\033[1;97m[\033[1;92m+\033[1;97m] TOOL PASSWORD: ")
	if passw =="brand":
	    os.system('clear')
